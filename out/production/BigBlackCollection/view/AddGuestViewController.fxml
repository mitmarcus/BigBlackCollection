<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AddGuestViewController">
   <children>
      <HBox alignment="CENTER_LEFT" minHeight="-Infinity" prefHeight="60.0" prefWidth="200.0">
         <children>
            <Button onAction="#returnButtonPressed" mnemonicParsing="false" text="Return to main menu" />
         </children>
         <padding>
            <Insets left="45.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" prefHeight="180.0" prefWidth="200.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add Guest">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="110.0" prefWidth="200.0">
         <children>
            <TextField fx:id="firstNameText" prefHeight="26.0" prefWidth="140.0" promptText="First Name">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="lastNameText" prefHeight="26.0" prefWidth="140.0" promptText="Last Name">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="phoneText" layoutX="231.0" layoutY="52.0" prefHeight="26.0" prefWidth="158.0" promptText="Phone number">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></TextField>
            <Button mnemonicParsing="false" text="Add guest" onAction="#addMember" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="190.0" prefHeight="110.0" prefWidth="200.0" />
   </children>
</VBox>
